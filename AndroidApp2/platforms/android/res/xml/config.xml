    <feature name="GDXMLHttpRequest">
        <param name="android-package" value="com.good.gd.cordova.plugins.GDXmlHttpRequestPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>HelloWorld</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="hooks/afterBasePluginRemove.js" type="after_plugin_rm" />
    <hook src="hooks/android/afterCordovaPluginInstall.js" type="after_plugin_install" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="25" />
