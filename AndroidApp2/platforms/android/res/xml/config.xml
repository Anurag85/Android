    <feature name="GDInterAppCommunication">
        <param name="android-package" value="com.good.gd.cordova.plugins.GDInterAppCommunicationPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BBDMailToPlugin">
        <param name="android-package" value="com.good.gd.cordova.plugins.BBDMailToPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GDPush">
        <param name="android-package" value="com.good.gd.cordova.plugins.push.GDPushPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GDServerSideServices">
        <param name="android-package" value="com.good.gd.cordova.plugins.GDServerSideServicesPlugin" />
