<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wellsfargo.internalapps.gd.android" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloWorld</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <hook src="hooks/android/afterCordovaPluginInstall.js" type="after_plugin_install" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <hook src="hooks/afterBasePluginRemove.js" type="after_plugin_rm" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-targetSdkVersion" value="25" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-bbd-all" spec="file:../BlackBerry_Dynamics_SDK_for_Cordova_v4.0.0.182/plugins/cordova-plugin-bbd-all" />
    <plugin name="cordova-plugin-bbd-base" spec="file:../BlackBerry_Dynamics_SDK_for_Cordova_v4.0.0.182/plugins/cordova-plugin-bbd-base" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
